                <TextBox Height="23" HorizontalAlignment="Left" Margin="31,82,0,0" Name="textBox2" VerticalAlignment="Top" Width="230" Grid.Column="1" />
                <Label Content="Adress" Height="28" HorizontalAlignment="Left" Margin="12,80,0,0" Name="label6" VerticalAlignment="Top" />
                <TextBox Height="23" HorizontalAlignment="Left" Margin="31,113,0,0" Name="textBox8" VerticalAlignment="Top" Width="230" Grid.Column="1" />
