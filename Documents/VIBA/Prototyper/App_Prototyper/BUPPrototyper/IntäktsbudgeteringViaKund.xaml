        <Button Content="Skriv ut" Height="23" HorizontalAlignment="Left" Margin="110,408,0,0" x:Name="button5" VerticalAlignment="Top" Width="73" />
        <Button Content="Export av textfil" Height="23" HorizontalAlignment="Left" Margin="10,408,0,0" x:Name="button6" VerticalAlignment="Top" Width="95" />
        <Button Content="Lås budget" Height="23" HorizontalAlignment="Left" Margin="682,10,0,0" x:Name="button7" VerticalAlignment="Top" Width="73" />
        <ComboBox Height="23" HorizontalAlignment="Right" Margin="0,10,464,0" x:Name="comboBox1" SelectedIndex="0" Text="Välj..." VerticalAlignment="Top" Width="224">
            <ComboBoxItem Content="Välj..." />
        </ComboBox>
        <Label Content="Välj kund" Height="28" HorizontalAlignment="Left" Margin="10,10,0,0" x:Name="label1" VerticalAlignment="Top" Width="62" />

    </Grid>
</Window>
