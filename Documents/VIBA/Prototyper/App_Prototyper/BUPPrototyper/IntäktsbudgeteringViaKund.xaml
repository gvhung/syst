                <DataGridTextColumn Header="Namn" MinWidth="80" Width="120" />
                <DataGridTextColumn Header="Avtal" MinWidth="40" />
                <DataGridTextColumn Header="Grad A" MinWidth="40" />
                <DataGridTextColumn Header="Tillägg" MinWidth="40" />
                <DataGridTextColumn Header="Grad T" MinWidth="40" />
                <DataGridTextColumn Header="Budget" MinWidth="40" />
                <DataGridTextColumn Header="Tim" MinWidth="40" />
                <DataGridTextColumn Header="Kommentarer" MinWidth="80" />
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Avbryt" Height="23" HorizontalAlignment="Left" Margin="604,408,0,0" x:Name="button1" VerticalAlignment="Top" Width="73" />
        <Button Content="Spara" Height="23" HorizontalAlignment="Right" Margin="0,408,10,0" x:Name="button2" VerticalAlignment="Top" Width="73" />
        <Button Content="Exportera till excel" Height="23" HorizontalAlignment="Left" Margin="188,408,0,0" x:Name="button3" VerticalAlignment="Top" Width="113" />
